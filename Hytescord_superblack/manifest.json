{
	"name": "HytesCord SuperBlack",
	"description": "This browser extension enables the Discord mobile AMOLED Super Black theme in your discord browser, made by HyTera Development",
	
	"short_name": "SuperBlack",
	"version": "3.0",

	"storage": {
		"managed_schema": "superblack_assets/Files/storageFiles/schema.json"
	  },

	"manifest_version": 3,
	"icons": {
		"48": "superblack_assets/images/Themr-icon-48p.png",
		"128": "superblack_assets/images/Themr-icon-128p.png"
	  },
	"permissions": [
		"storage",
		"activeTab",
		"scripting"
	],
	"offline_enabled": true,
	"action": {
		"default_popup": "superblack_assets/Files/popup.html"
	},

	"author": "HyTera Development"
}
